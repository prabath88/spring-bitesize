project: prabath
environments:
  - name: dev
    namespace: prabath-dev
    deployment:
      method: rolling-upgrade
    services:
      - name: spring-app
        external_url: prabathtest.prsn.io
        port: 8080
        ssl: "false"
        replicas: 2
   - name: stg
    namespace: prabath-stg
    deployment:
      method: rolling-upgrade
    services:
      - name: spring-app
        external_url: prabathstg.prsn.io
        port: 8080
        ssl: "false"
        replicas: 2
